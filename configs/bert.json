{
    "name": "C_TSP_SY_classic",
    "dataset": {
        "records_path": "data/processed/14th_records.pkl",
        "test_pc": 0.3,
        "val_pc": 0.3,
        "label_var": "label",
        "text_vars": ["intervention", "titre_complet", "profession"],
        "feature_vars": ["n_sexe", "n_y_naissance"],
        "bert_type": "bert",
        "batch_size": 96,
        "num_workers": 12,
        "prefetch_factor": 2,
        "shuffle": true,
        "pin_memory": true,
        "drop_last": true,
        "random_state": 42
    },
    "classifier": {
        "name": "C_TSP_SY_classic",
        "linear_layers": {
            "layers": {
                "intervention": {
                    "bert_type": "bert",
                    "frozen": true,
                    "linear_dim": 0
                },
                "titre_complet": {
                    "bert_type": "bert",
                    "frozen": true,
                    "linear_dim": 0
                },
                "profession": {
                    "bert_type": "bert",
                    "frozen": true,
                    "linear_dim": 0
                }
            }
        },
        "feature_mlp": {
            "mlp_dims": [2, 768],
            "dropout": 0.0,
            "negative_slope": 0.0,
            "batch_norm": false
        },
        "pooler_layer": {
            "concat_type": "mean"
        },
        "mlp_layer": {
            "mlp_dims": [768, 128, 128, 3],
            "dropout": 0.0,
            "negative_slope": 0.01,
            "batch_norm": true
        }
    }
}
